{
  "expo": {
    "name": "Prana",
    "description": "Cross-platform mobile application for travel & geolocation documentation",
    "slug": "prana-app",
    "privacy": "public",
    "version": "1.0.0",
    "sdkVersion": "42.0.0",
    "platforms": [
      "ios", 
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/map.png",
    "splash": {
      "image": "./assets/prana_splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/map.png" 
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_map.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
