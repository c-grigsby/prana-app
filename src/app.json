"expo": {
  "name": "Prana",
  "description": "Cross-platform mobile application for travel & geolocation documentation",
  "slug": "prana-app",
  "privacy": "public",
  "version": "1.0.0",
  "sdkVersion": "42.0.0",
  "plugins": [
    [
      "expo-image-picker",
      {
        "photosPermission": "Allow Prana to access your photos to add to locations."
      }
    ]
  ],
  "platforms": [
    "ios", 
    "android"
  ],
  "orientation": "portrait",
  "icon": "./assets/map.png",
  "splash": {
    "image": "./assets/prana_splash.png",
    "resizeMode": "cover"
  },
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": ["**/*"],
  "ios": {
    "bundleIdentifier": "com.grigsby9.prana",
    "buildNumber": "1.0.0",
    "supportsTablet": true,
    "icon": "./assets/map.png" 
  },
  "android": {
    "package": "com.grigsby9.prana",
    "versionCode": 1,
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive_map.png",
      "backgroundColor": "#FFFFFF"
    }, 
    "permissions": [
      "ACCESS_FINE_LOCATIONS", 
      "CAMERA", 
      "WRITE_EXTERNAL_STORAGE", 
      "READ_EXTERNAL_STORAGE"
    ], 
    "config": {
      "googleMaps": {
        "apiKey": "GOOGLE_API_KEY"
      }
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  }
}
}
